{
  "name": "didery.js",
  "version": "1.0.0",
  "description": "Javascript client library for didery.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require babel-core/register --require babel-polyfill ./tests",
    "before": "./tests/before.sh",
    "after": "./tests/after.sh",
    "test-script": "./tests/test.sh",
    "dev": "npx webpack --mode development",
    "watch": "npx webpack -w --mode development",
    "build": "npx webpack --mode production",
    "dashboard": "npx webpack ./examples/dashboard/dashboard.js --output ./examples/dashboard/index.js --mode development",
    "sse": "npx webpack ./examples/sse/sse.js --output ./examples/sse/index.js --mode development"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "author": "Brady Hammond",
  "license": "ISC",
  "dependencies": {
    "file-saver": "^1.3.8",
    "isomorphic-fetch": "^2.2.1",
    "libsodium": "^0.7.3",
    "libsodium-wrappers": "^0.7.3",
    "moment": "^2.22.2",
    "npm": "^6.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jquery": "^3.3.1",
    "jsdom": "^11.11.0",
    "mithril": "^1.1.6",
    "mocha": "^5.2.0",
    "semantic-ui-css": "^2.3.1",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  }
}
